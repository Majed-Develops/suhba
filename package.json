{
	"name": "suhba-app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"backend": "cd server && poetry run python app.py",
		"backend:dev": "cd server && poetry run python -m uvicorn app:app --reload --host 0.0.0.0 --port 3005",
		"full:dev": "concurrently \"npm run dev\" \"npm run backend:dev\""
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"autoprefixer": "^10.4.21",
		"concurrently": "^9.1.0",
		"postcss": "^8.5.6",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.1.10",
		"typescript": "^5.0.0",
		"vite": "^6.2.6"
	},
	"dependencies": {
		"@sveltejs/adapter-static": "^3.0.8",
		"@tailwindcss/postcss": "^4.1.10",
		"lucide-svelte": "^0.522.0"
	}
}
